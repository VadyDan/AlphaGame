<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="4dip" android:color="#e66060" />
            <solid android:color="#b7d65a"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="1dip" android:color="#acb5c4" />
            <solid android:color="#e66060"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="2dip" android:color="#f0ffed" />
            <solid android:color="#d9ff69"/>
        </shape>
    </item>
</selector>
